<h1>New Application</h1>

<div id="new_application">
  <%= form_with url: "/applications", method: :post, local: true do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>

    <br>

    <%= f.label :street_address %>
    <%= f.text_field :street_address %>

    <br>

    <%= f.label :city %>
    <%= f.text_field :city %>

    <br>

    <%= f.label :state %>
    <%= f.text_field :state %>

    <br>

    <%= f.label :zip %>
    <%= f.text_field :zip %>

    <br>

    <%= f.label :description %>
    <%= f.text_area :description %>

    <br>

    <%= f.submit %> 
  <% end %>
</div>